target 'PictureSharingApp' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for PictureSharingApp
  pod 'AWSCore'
  pod 'AWSCognito'
  pod 'AWSS3'
  pod 'AWSSNS'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'

  target 'PictureSharingAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PictureSharingAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
